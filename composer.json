{
  "name": "voku/webdev-news",
  "type": "project",
  "description": "Web-Developer News (RSS -> Plaintext)",
  "keywords": [
    "webdev"
  ],
  "homepage": "http://github.com/voku/webdev-news",
  "license": "MIT",
  "authors": [
    {
      "name": "Lars Moelleken",
      "email": "lars@moelleken.org"
    }
  ],
  "config": {
    "optimize-autoloader": true,
    "vendor-dir": "vendor/composer/",
    "github-protocols": [
      "git",
      "https",
      "http"
    ]
  },
  "autoload": {
    "psr-0": {
      "webdev": "src/"
    }
  },
  "require": {
    "php": ">=5.3.0",
    "voku/simple_html_dom": "~2.0.15",
    "voku/php-domain-parser": "~5.0.2",
    "voku/Arrayy": "~2.2.0",
    "voku/Stringy": "~2.2.25",
    "voku/html2text": "~3.4.0",
    "voku/httpful": "~0.2.25",
    "voku/anti-xss": "^1.2.14",
    "voku/simple-cache": "^2.0.12",
    "voku/simple-mysqli": "^1.8.27",
    "voku/paris": "^1.7.4",
    "voku/kint": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0"
  }
}
